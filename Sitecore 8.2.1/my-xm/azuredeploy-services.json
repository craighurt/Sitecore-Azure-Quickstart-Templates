{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "variables": {},
  "parameters": {
    "sqlserverLogin": {
      "type": "string",
      "minLength": 1
    },
    "sqlserverPassword": {
      "type": "securestring",
      "minLength": 8
    }
  },
  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "azuredeploy-services",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/mathieu-benoit/Sitecore-Azure-Quickstart-Templates/master/Sitecore%208.2.1/my-xm/azuredeploy-nested-services.json"
        },
        "parameters": {
          "sqlserverLogin": {
            "value": "[parameters('sqlserverLogin')]"
          },
          "sqlserverPassword": {
            "value": "[parameters('sqlserverPassword')]"
          }
        }
      }
    }
  ]
}